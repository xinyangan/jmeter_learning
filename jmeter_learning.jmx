          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell 预处理程序" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">//beanshell是一种符合java语法规范的脚本语言，并且又拥有自己的一些语法和方法
//是一种嵌入式java源代码解释器

//定义jmeter变量
//vars.put(&quot;user&quot;,&quot;axy&quot;);
//日志输出
//log.info(&quot;生日快乐&quot;);

//生成时间
import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.util.Data;

Date date = new Date();
SimpleDateFormat sf = new SimpleDateFormat(&quot;yyyy-MM-dd&quot;);
//创建格式化的字符串以及连接多个字符串对象
String nowDate = sf.format(date);
//获取指定时间点
Calendar cal = Calendar.getInstance();
//用于使用此Calendar的指定Date(d)设置时间
//cal.setTime(sf.parse(nowDate));
cal.add(Calendar.DAY_OF_YEAR,+3);
String chanceDate = sf.format(cal.getTime());
cal.add(Calendar.DAY_OF_YEAR,+7);
String planFinishDate = sf.format(cal.getTime());
//创建参数，其他地方可以${}引用
vars.put(&quot;orderDate&quot;,chanceDate);
vars.put(&quot;delivery&quot;,planFinishDate);
</stringProp>
          </BeanShellPreProcessor>
